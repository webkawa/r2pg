<?xml version="1.0" encoding="UTF-8"?>
<!-- Framework native errors -->
<catalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="../../data/models/errors.xsd" 
         name="log" >
    
    <error code="0">
        <message>Unknow log error</message>
    </error>
    
    <error code="1">
        <message>Log-writing entity doesn't implement "getLogID" correctly</message>
        <param id="entity" required="true">Entity</param>
    </error>
    
    <error code="2">
        <message>Already used log ID detected</message>
        <param id="id">Log ID</param>
        <param id="entity">Entity</param>
    </error>
    
    <error code="3">
        <message>Unable to find log writer by ID</message>
        <param id="id" required="true">Log ID</param>
    </error>
    
</catalog>
