<?xml version="1.0" encoding="UTF-8"?>
<!-- Framework native errors -->
<catalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="../../data/models/errors.xsd" 
         name="cpn" >
    
    <error code="0">
        <message>Unknow component error</message>
    </error>
    <error code="1" critical="true">
        <message>Error during method execution</message>
        <param required="required" id="name">Method name</param>
        <param required="required" id="context">Context</param>
        <param id="params">Parameters</param>
    </error>
    <error code="2" critical="true">
        <message>Call to undefined method</message>
        <param required="true" id="component">Component ID</param>
        <param required="true" id="name">Method name</param>
    </error>
   <error code="3">
        <message>Try to rewrite an already defined source</message>
        <param required="true" id="component">Component ID</param>
        <param required="true" id="name">Source name</param>
    </error>
    <error code="4" critical="true">
        <message>Unable to load model at specified URL</message>
        <param required="true" id="url">URL</param>
        <param id="info">Error info</param>
    </error>
    <error code="5">
        <message>Try to access an inactive selector</message>
        <param required="true" id="name">Name</param>
        <param required="true" id="path">Path</param>
    </error>
    <error code="6">
        <message>Unable to find the requested selector</message>
        <param required="true" id="name">Name</param>
    </error>
    <error code="7">
        <message>Try to rewrite a locked method</message>
        <param required="true" id="component">Component ID</param>
        <param required="true" id="name">Method name</param>
    </error>
    <error code="8">
        <message>Try to add an already defined selector</message>
        <param required="true" id="name">Name</param>
    </error>
    <error code="9">
        <message>Try to set an invalid component state</message>
        <param required="true" id="component">Component ID</param>
        <param required="true" id="state">State</param>
    </error>
    <error code="10">
        <message>Try to start an already started component</message>
        <param required="true" id="component">Component ID</param>
    </error>
    <error code="11">
        <message>Call for invalid sequence target</message>
        <param required="true" id="component">Component ID</param>
        <param required="true" id="target">Target</param>
    </error>
    <error code="12">
        <message>Failed to call method</message>
        <param required="true" id="name">Method</param>
        <param id="component">Component ID</param>
    </error>
    <error code="13">
        <message>Animation failed</message>
    </error>
    <error code="14">
        <message>Error during state change</message>
        <param required="true" id="component">Component ID</param>
    </error>
    <error code="15">
        <message>Invalid transition</message>
        <param required="true" id="component">Component ID</param>
        <param id="error">Error</param>
    </error>
    <error code="16">
        <message>Invalid component ID</message>
        <param required="true" id="id">ID</param>
        <param id="error">Error</param>
    </error>
    <error code="17">
        <message>Component container CSS position must be relative</message>
        <param required="true" id="id">Component ID</param>
        <param id="position">CSS Position</param>
    </error>
    <error code="18">
        <message>Error during data source loading</message>
        <param required="true" id="url">URL</param>
        <param id="info">Error info</param>
    </error>
    <error code="19">
        <message>Unable to find data source</message>
        <param required="true" id="component">Component ID</param>
        <param required="true" id="source">Source name</param>
    </error>
    <error code="20" critical="true">
        <message>Data source native error</message>
        <param required="true" id="component">Component ID</param>
        <param required="true" id="source">Source name</param>
    </error>
    <error code="21">
        <message>Invalid delayed-for-removal tag (component is ready)</message>
        <param required="true" id="component">Component ID</param>
        <param required="true" id="status">Component status</param>
    </error>
    <error code="22">
        <message>Try to access data source while starting</message>
        <param required="true" id="component">Component ID</param>
        <param required="true" id="source">Source name</param>
    </error>
    <error code="23">
        <message>Catch error during delayed method call</message>
        <param required="true" id="component">Component ID</param>
        <param required="true" id="method">Method name</param>
    </error>
    <error code="24">
        <message>Invalid validator type</message>
        <param required="true" id="type">Type</param>
    </error>
    <error code="25">
        <message>Invalid violation message</message>
        <param id="variable">Missing variable</param>
        <param id="type">Type</param>
        <param required="true" id="message">Message</param>
    </error>
    <error code="26">
        <message>Invalid trajectory</message>
        <param required="true" id="component">Component ID</param>
        <param required="true" id="trajectory">Trajectory</param>
    </error>
    
</catalog>
