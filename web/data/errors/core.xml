<?xml version="1.0" encoding="UTF-8"?>
<!-- Framework native errors -->
<catalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="../../data/models/errors.xsd" 
         name="core" >
    
    <error code="0">
        <message>Unknow core error</message>
    </error>
    
    <error code="1" critical="true">
        <message>Undefined error catched</message>
        <param id="cause">Cause</param>
        <param id="context">Catalog</param>
        <param id="id">ID</param>
    </error>
    
    <error code="2">
        <message>Native JS error catched</message>
        <param id="name" required="true">Name</param>
        <param id="message" required="true">Message</param>
    </error>
    
    <error code="3">
        <message>Invalid error catalog URL, load fails</message>
        <param id="url">URL</param>
        <param id="info">Error</param>
    </error>
    
    <error code="4">
        <message>Missing error parameter</message>
        <param id="id">ID</param>
        <param id="parameter">Parameter</param>
    </error>
    
</catalog>
